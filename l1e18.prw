#INCLUDE "TOTVS.CH"

User Function JsL1e18()

Local nNumero     := ""

//Insira um numero
While nNumero <= 10
    FwAlertInfo(cValtochar(nNum))
    nNum++
ENDDO

//Mostre nAntecessor
FwAlertSuccess('Contagem Finalizada')

RETURN
